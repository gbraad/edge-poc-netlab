<network>
  <name>default</name>
  <forward mode='route'> </forward>
  <bridge name='br0' stp='on' delay='0'/>
  <ip address='{{ netlab_libvirt_default_dhcp_address }}' netmask='{{ netlab_libvirt_default_dhcp_mask }}'>
    <dhcp>
      <range start='{{ netlab_libvirt_default_dhcp_start }}' end='{{ netlab_libvirt_default_dhcp_end }}'/>
      <host mac='52:54:00:cc:cc:01' ip='{{ netlab_default_ip_ios01 }}'/>
      <host mac='52:54:00:aa:aa:01' ip='{{ netlab_default_ip_veos01 }}'/>
    </dhcp>
  </ip>
</network>

